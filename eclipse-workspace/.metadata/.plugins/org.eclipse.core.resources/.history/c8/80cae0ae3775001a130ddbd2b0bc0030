#include <stdio.h>
#include <stdlib.h>
#include "utn.h"

int main()
{
	int n;
	setbuf(stdin, NULL);
	utn_getNumero(&n, "ingrese el numero:", "error", 1, 9, 3);
	printf("num ingresado:%d", n);

	return 0;
}


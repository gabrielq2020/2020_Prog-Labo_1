#include <stdio.h>
#include <stdlib.h>

void funcion(void);

int main(void) {
	setbuf(stdout, NULL);

	funcion();

	return 0;
}

void funcion(void){
	int numeroIngresado;
	int numeroPantalla;
	printf("Ingrese un numero");
	fflush(stdin);
	scanf("%d", &numeroIngresado);
	printf("El numero que ingreso es: %d", numeroPantalla);
	return numeroPantalla;
}

#include <stdio.h>
#include <stdlib.h>

int main()
{

setbuf(stdout, NULL);

int nota;
int res;

nota=9;

res= (nota >= 7);

printf("Contenido de la variable res %d", res);

if(res)
{
	printf("\n El alumno esta aprobado \n");
}

return 0;

}
